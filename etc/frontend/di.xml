<?xml version='1.0'?>
<config
	xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:noNamespaceSchemaLocation='urn:magento:framework:ObjectManager/etc/config.xsd'
>
	<type name='Magento\Framework\App\RouterList'>
		<arguments>
			<argument name='routerList' xsi:type='array'>
				<item name='dfe-cms' xsi:type='array'>
					<item name='class' xsi:type='string'>Dfe\Cms\Router</item>
					<item name='disable' xsi:type='boolean'>false</item>
<!-- 2017-05-04
This router will be called before all the built-in routers.
«The built-in frontend routers»: https://mage2.pro/t/3878
«How is the CMS frontend router (Magento\Cms\Controller\Router) implemented?» https://mage2.pro/t/3879
-->
					<item name='sortOrder' xsi:type='string'>19</item>
				</item>
			</argument>
		</arguments>
	</type>
</config>